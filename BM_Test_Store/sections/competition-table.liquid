<div class="competition-table">
  <h3 style="text-align: center;">{{ section.settings.compheading }}</h3>
  <table >
    {% for block in section.blocks %}

<tr>
  <td>{{ block.settings.tdone }}</td>
   <td>{{ block.settings.tdtwo }}</td>
    <td>{{ block.settings.tdthree }}</td>
    <td>{{ block.settings.tdfour }}</td>
    <td>{{ block.settings.tdfive }}</td>
</tr>
      {% endfor %}
  </table>
  <p class="table-dic">{{ section.settings.compdesc }}</p>
</div>
{% assign productPillow = section.settings.product-id %}
{% comment %} {%  for variant in productPillow.variants %} 
    {{ variant.title }}
{{ variant.price | money_without_trailing_zeros |  replace: '$',''  }}
{% endfor %} {% endcomment %}


<style>
  .table-dic{
    color: #667085;
  font-family: apercu, Helvetica, Arial, sans-serif;
font-size: 12px;
font-weight: 300;
line-height: 16px;
text-align: center;
margin: 0;
    margin-top: 1rem;
  }
  .competition-table{
    padding: 48px 0;
  }
  .competition-table table{
    max-width: 768px;
    margin: auto;
     border-collapse: collapse;
  }
   .competition-table table tr{
     border: none;
   }
   .competition-table td {
    text-align: center!important;
    font-family: apercu, Helvetica, Arial, sans-serif;
font-size: 14px;
font-weight: 400;
line-height: 18px;
text-align: left;
         white-space: pre;
  }
 .competition-table tr td:first-child {
    text-align: left!important;
  }
  .competition-table tr:first-child td:nth-child(2) {
    background-color: #3B82FF;
    color: #fff;
    font-size: 16px;
       border-top: 1.5px solid #3B82FF;
       border-top-left-radius: 4px;
    border-top-right-radius: 4px;
  }
  .competition-table td:nth-child(2) {
    border-right: 1.5px solid #3B82FF;
      border-left: 1.5px solid #3B82FF;
  }
 .competition-table tr:last-child td:nth-child(2) {
    border-bottom: 1.5px solid #3B82FF;
  }
 .competition-table tr td:first-child {
   border-left: 1px solid #475467;
  }
  .competition-table tr:first-child td:first-child,
  .competition-table tr:last-child td:first-child {
    border-left: none;
  }
 .competition-table tr:nth-child(2) td:first-child {
    border-top: 1px solid #475467;
  }
 .competition-table tr:nth-last-child(2) td:first-child {
    border-left: 1px solid #226796;
    border-top: 1px solid #226796;
    border-bottom: 1px solid #226796;
