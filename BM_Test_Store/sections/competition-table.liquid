function appendTr(){
// Create a new row element
var newRow = document.createElement('tr');

// Insert the HTML content for the new row
newRow.innerHTML = `
<td>Celliant SRT<br>Infused Cover</td>
<td><p style="color: #30D686"><i class="mdi mdi-checkbox-marked-circle table-icon-md" style="text-shadow:none; font-weight:600"></i></p></td>
<td><i class="mdi mdi-window-close table-icon-sm" style="text-shadow:none; font-weight:600"></i></td>
<td><i class="mdi mdi-window-close table-icon-sm" style="text-shadow:none; font-weight:600"></i></td>
<td><i class="mdi mdi-window-close table-icon-sm" style="text-shadow:none; font-weight:600"></i></td>
`;

// Select the table body where you want to append the new row
var tableBody = document.querySelector('tbody');

// Get the third-to-last row in the table
var thirdLastRow = tableBody.rows[tableBody.rows.length - 2];
var newRowremove = tableBody.rows[tableBody.rows.length - 3];

// Insert the new row before the third-to-last row
var valSelect = $('#cs-option-selection-1').val();
if(valSelect == "Yes"){
  tableBody.insertBefore(newRow, thirdLastRow);
  }
  else{
    tableBody.removeChild(newRowremove)
  }
}
$('#cs-option-selection-1').on('change', function () {
  appendTr()
});
</script>