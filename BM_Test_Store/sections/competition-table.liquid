{% schema %}
  {
  "name": "Competition Table",
    "settings": [
            {
            "type": "text",
            "id": "compheading",
            "label": "Section Heading"
            },
            {
                "type": "product",
                "id": "product-id",
                "label": "Pillow Product"
            },
            {
                "type": "product",
                "id": "product-protector",
                "label": "Protector Product"
            },
            {
                "type": "product",
                "id": "product-sheet",
                "label": "Sheet Product"
            },
            {
            "type": "text",
            "id": "compdesc",
            "label": "Description below table"
            }
        ],
     "blocks": [
     {
       "name": "Row",
       "type": "Row",
       "settings": [
       
           {
      "type": "text",
      "id": "tdone",
      "label": "Text for TD One",
      "default": "Preset Text for TD One"
    },
    {
      "type": "html",
      "id": "tdtwo",
      "label": "Text for TD Two"
    },
    {
      "type": "text",
      "id": "tdthree",
      "label": "Text for TD Three",
      "default": "Preset Text for TD Three"
    },
    {
      "type": "text",
      "id": "tdfour",
      "label": "Text for TD Four",
      "default": "Preset Text for TD Four"
    },
    {
      "type": "text",
      "id": "tdfive",
      "label": "Text for TD Five",
      "default": "Preset Text for TD Five"
    }
       ]
     }
  ],
     "presets": [
    {
      "name": "Competition Table"
    }
  ]
}

{% endschema %}
