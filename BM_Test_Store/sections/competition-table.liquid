    color: #475467;
    font-weight: 800;
  }
  .competition-table tr:nth-child(2) td:nth-child(3),
.competition-table tr:nth-child(2) td:nth-child(4),
 .competition-table tr:nth-child(2) td:nth-child(5) {
    border-top: 1px solid #475467;
  }
 .competition-table tr td:last-child {
   border-right: 1px solid #475467;
  }
  .competition-table tr:last-child td:last-child,
  .competition-table tr:first-child td:last-child {
    border-right: none;
  }
 .competition-table table tr:nth-last-child(2) td:nth-child(5) {
    border-bottom: 1px solid #226796;
    border-right: 1px solid #226796;
    border-top: 1px solid #226796;
    background-color: #F6FAFF;
    color: #475467;
    font-weight: 800;
}
 .competition-table table tr:nth-last-child(2) td:nth-child(4) {
    border-bottom: 1px solid #226796;
    border-top: 1px solid #226796;
    background-color: #F6FAFF;
    color: #475467;
     font-weight: 800;
}
   .competition-table table tr:nth-last-child(2) td:nth-child(3) {
    border-bottom: 1px solid #226796;
    border-top: 1px solid #226796;
    background-color: #F6FAFF;
      color: #475467;
       font-weight: 800;
}
 .competition-table table td p{
       margin: 0;
    align-items: center;
    display: flex;
    justify-content: center;
    font-weight: 800;
    color: #0C324C;
  }
   .competition-table table td span{
     margin-left: 0px;
     text-decoration: line-through;
     color: #96979c;
     font-weight: 100;
    }
    .competition-table table td span.dark-text{
      color: #000;
      text-decoration: none;
    }
  .competition-table tr:nth-child(6) td:nth-child(3),
.competition-table tr:nth-child(6) td:nth-child(4),
.competition-table tr:nth-child(6) td:nth-child(5) {
    border-radius: 4px;
}
.checkbox-icon-tables{
    color: #30D686;
}
.close-icon-table{
    color: #475467;
;
}
  @media only screen and (max-width: 600px) {
  .competition-table td {
   padding: 8px;
  }
   .competition-table table {
    width: 91%;
    }
}
